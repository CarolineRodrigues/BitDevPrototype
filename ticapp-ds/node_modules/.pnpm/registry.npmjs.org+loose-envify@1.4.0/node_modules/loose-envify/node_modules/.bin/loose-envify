#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Utilizador/Documents/ticapp-ds/node_modules/.pnpm/registry.npmjs.org+loose-envify@1.4.0/node_modules/loose-envify/node_modules:/mnt/c/Users/Utilizador/Documents/ticapp-ds/node_modules/.pnpm/registry.npmjs.org+loose-envify@1.4.0/node_modules:/mnt/c/Users/Utilizador/Documents/ticapp-ds/node_modules/.pnpm/node_modules:/mnt/c/Users/Utilizador/Documents/ticapp-ds/node_modules:/mnt/c/Users/Utilizador/Documents/node_modules:/mnt/c/Users/Utilizador/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/Utilizador/Documents/ticapp-ds/node_modules/.pnpm/registry.npmjs.org+loose-envify@1.4.0/node_modules/loose-envify/node_modules:/mnt/c/Users/Utilizador/Documents/ticapp-ds/node_modules/.pnpm/registry.npmjs.org+loose-envify@1.4.0/node_modules:/mnt/c/Users/Utilizador/Documents/ticapp-ds/node_modules/.pnpm/node_modules:/mnt/c/Users/Utilizador/Documents/ticapp-ds/node_modules:/mnt/c/Users/Utilizador/Documents/node_modules:/mnt/c/Users/Utilizador/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
